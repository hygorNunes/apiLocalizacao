{
  "name": "apiTeste",
  "version": "1.0.0",
  "description": "Api",
  "private": false,
  "repository": "https://github.com/hygorNunes/apiTeste/",
  "author": "",
  "homepage": "https://dostawa.well.eti.br/",
  "copyright": "Copyright 2019 Well Tecnologia",
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "nodemon src/app --exec \"node -r babel-register\"",
    "start:dev": "nodemon src/app --exec \"npm run lint && node -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir dist && babel src/ -s -d dist",
    "test": "mocha --exit --require babel-register test/",
    "lint": "esw src",
    "semantic-release": "semantic-release -e @semantic-release/gitlab -D"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "config": "^1.31.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^2.6.9",
    "dotenv": "^8.1.0",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "express-swagger-generator": "^1.1.15",
    "express-validator": "^6.2.0",
    "js-md5": "^0.7.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.28.0",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "node-require-context": "^1.0.0",
    "mysql2": "^2.0.0",
    "nodemailer": "^6.4.6",
    "nodemon": "^1.19.2",
    "rimraf": "^2.7.1",
    "sequelize": "^5.22.3",
    "sequelize-auto-migrations": "^1.0.3",
    "sequelize-querystring": "^0.10.0",
    "tedious": "8.0.1",
    "uuid": "^8.3.0",
    "validatorjs": "^3.19.2"
  },
  "keywords": [
    "API",
    "Rest",
    "RestFull",
    "Framework",
    "OpenSource",
  ],
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^4.12.1",
    "eslint-watch": "^3.1.5",
    "mocha": "^4.1.0",
    "sequelize-cli": "^5.5.1",
    "supertest": "^3.4.2"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/gitlab"
    ]
  }
}